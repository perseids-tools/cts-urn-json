[
  {
    "transform": {
      "title": ["Machine readable text", ""]
    }
  },
  {
    "transform": {
      "title": ["\\(English\\)\\.?", ""]
    }
  },
  {
    "transform": {
      "title": ["\\(ENGLISH\\)\\.?", ""]
    }
  },
  {
    "transform": {
      "title": ["\\(Greek\\)\\.?", ""]
    }
  },
  {
    "transform": {
      "author": ["\\s+\\z", ""],
      "title": ["\\s+\\z", ""]
    }
  },
  {
    "transform": {
      "author": ["\\s+", " "],
      "title": ["\\s+", " "]
    }
  },
  {
    "match": {
      "author": "\\A\\z"
    },
    "transform": {
      "author": ["", "Unknown"]
    }
  },
  {
    "match": {
      "title": "\\A\\z"
    },
    "transform": {
      "title": ["", "Unkown"]
    }
  }
]
